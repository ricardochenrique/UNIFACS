import java.util.Scanner;
public class NumerosImparesdeumIntervalo {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		System.out.println("IMPRIMA NÚMEROS IMPARES DE UM INTERVALO [a,b]");
		System.out.println("Digite o número inferior do intervalo: ");
		int a = sc.nextInt();
		
		System.out.println("Informe o número superior do intervalo: ");
		int b = sc.nextInt();
		
		System.out.println("Os números impares do intervalo são: ");
		for (int i = a; i <= b; i++) {
			if (i%2!=0) {
				System.out.println(i);
		
				
			}
			
		}

	}
	

}
