package aplicacao;
import java.util.Scanner;
public class Vetores {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int[]vet = new int[10]; //CRIANDO VETOR
		int soma = 0;
		System.out.println(" Digite 10 notas: ");

		for ( int i = 0; i < 10; i++) {
			System.out.println("Informe o nÃºmero " + (i + 1));
			vet [i] = sc.nextInt();
			soma += vet [i];
		}

		System.out.println(" O resultado = " + soma);
		sc.close();
	}

}


